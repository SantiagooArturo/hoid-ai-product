{
  "manifest_version": 3,
  "name": "Audio Analyzer Tab Selector",
  "version": "1.0",
  "description": "Select browser tabs for audio recording and analysis",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
} 